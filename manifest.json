  {
    "name": "Reskipe",
    "version": "1.0",
    "description": "Skip the nonsense, get right to the recipe!",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "placeHolder.png"
     },
     "permissions": [
      "activeTab"
      ],
      "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["popup.js"]
        }
      ]
  }